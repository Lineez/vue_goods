{
    "name": "vue-default",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve --open --host localhost",
        "build": "vue-cli-service build",
        "lint": "vue-cli-service lint",
        "gh-pages": "yarn build && git add dist -f && git commit -m 'adding dist subtree' && git subtree push --prefix dist origin gh-pages"
    },
    "dependencies": {
        "axios": "^0.26.1",
        "core-js": "^3.6.5",
        "vue": "^3.0.0",
        "vue-router": "^4.0.13",
        "vuex": "^4.0.0-0"
    },
    "devDependencies": {
        "@babel/core": "^7.12.16",
        "@babel/eslint-parser": "^7.12.16",
        "@vue/cli-plugin-babel": "~5.0.4",
        "@vue/cli-plugin-eslint": "~5.0.4",
        "@vue/cli-plugin-router": "~5.0.4",
        "@vue/cli-plugin-vuex": "~5.0.4",
        "@vue/cli-service": "~5.0.4",
        "@vue/compiler-sfc": "^3.0.0",
        "@vue/eslint-config-prettier": "^6.0.0",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^3.1.0",
        "eslint-plugin-vue": "^8.0.3",
        "prettier": "^2.4.1",
        "sass": "^1.49.10",
        "sass-loader": "^12.6.0"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/vue3-essential",
            "eslint:recommended",
            "@vue/prettier"
        ],
        "parserOptions": {
            "parser": "@babel/eslint-parser"
        },
        "rules": {}
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not dead"
    ]
}
